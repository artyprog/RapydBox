size(400,400)


background(0.1,0.1,0.1)
shadow(0,0.3,0)

translate(200,200)

stroke(0,0,0,0.15)
nostroke()



    
r = 2300
s = 5
for i in range(r):
    push()
    fill(1,1,1,i/(r+i/10))
    rotate(i)
    rect(random(-10,10),random(-10,10),random(40)+s-(s*i/r), random(1,-1)+s-(s*i/r))
    pop()

    
r = 1000
s = 0.89
for i in range(r):
    push()
    fill(0,0,0.3,i/(r+i/10))
    rotate(i)
    rect(random(-1,1),random(-5,5),random(9)+s-(s*i/r), + random(10)+s-(s*i/r))
    pop()