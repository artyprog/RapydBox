size(800,800)


background(0,0,0.01)
shadow(1,1.1,0)

translate(400,400)
stroke()




    
r = 9230
s = 500
for i in range(r):
    push()
    fill(random(1)+0.752,random(0.32503),0.1,i/(r+i/5))
    rotate(20*Math.log(i))
    scale(Math.random(3.0),1.0)
    oval(0,0,1.2*s-(s*i/r), random(10))
    pop()
      


    
