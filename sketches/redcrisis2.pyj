size(400,400)


background(0,0,0.01)
shadow(0,0.3,0)

translate(200,200)

stroke(0,0,0,0.15)
nostroke()



    
r = 2300
s = 120
for i in range(r):
    push()
    if i < 680:
        fill(random(1),random(0.03),0.1,i/(r+i/10))
    else:
        fill(1,0.96,0.6,(r-i)/r)
    rotate(i)
    scale((r*0.258)/i)
    oval(random(-100,100),random(-5,5),random(2)+s-(s*i/r), random(10))
    pop()
      


    
