size(400,400)


background(0,0,0.01)
shadow(0,0.3,0)

translate(200,200)

stroke(0,0,0,0.15)
nostroke()



    
r = 1668
s = 12
for i in range(r):
    push()
    fill(1,0.28,random(0.6),i/(r-i/100))
    rotate(i)
    scale(1100/(i+1000))
    oval(random(-10,10),random(-100,100),random(0.2)+s-(s*i/r), random(-0.2,0.2)+s-(s*i/r))
    pop()
      


    
